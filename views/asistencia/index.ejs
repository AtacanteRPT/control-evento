<h1>asistencia</h1>


<% if (result.length == 0) { %>
  <h4>
    0 asistencia
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>createdAt</th>
			<th>updatedAt</th>
			<th>id</th>
			<th>fecha</th>
			<th>estado</th>
			<th>hora</th>
			<th>idMilitante</th>
			<th>idPersonas</th>
			<th>idEvento</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.createdAt %>
					</td>
					<td>
						<%= item.updatedAt %>
					</td>
					<td>
						<%= item.id %>
					</td>
					<td>
						<%= item.fecha %>
					</td>
					<td>
						<%= item.estado %>
					</td>
					<td>
						<%= item.hora %>
					</td>
					<td>
						<%= item.idMilitante %>
					</td>
					<td>
						<%= item.idPersonas %>
					</td>
					<td>
						<%= item.idEvento %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/asistencia/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/asistencia/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/asistencia/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/asistencia/create">New</a>